      "int binarysrc(vector<int> v, int q)",
      "{",
      "    int first = 0;",
      "    int last = v.size() - 1;",
      "",
      "    while (first <= last)",
      "    {",
      "        int mid = (first + last) / 2;",
      "        if (q == v[mid])",
      "        {",
      "            return mid;",
      "        }",
      "        else if (v[mid] < q)",
      "        {",
      "            first = mid + 1;",
      "        }",
      "        else if (v[mid] > q)",
      "        {",
      "            last = mid - 1;",
      "        }",
      "    }",
      "    return -1;",
      "}",
